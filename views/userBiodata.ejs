<html>
<head>
    <title>Biodata | <%= users.username %></title>
</head>

<body>
    <form method="POST" action="/user/<%= users.id %>">
    <label for="first_name">First Name</label>
    <br>
    <input type="text" name="first_name" value="<%= userBio.first_name %>"/>
    <br>

    <label for="last_name">Last Name</label>
    <br>
    <input type="text" name="last_name" value="<%= userBio.last_name %>"/>
    <br>

    <label for="birthday">Birthday</label>
    <br>
    <input type="string" name="birthday" value= "<%= moment(userBio.birthday).format('YYYY-MM-DD')%>"
    />
    <br>
    <!-- <%= new Date(userBio.birthday).toLocaleDateString("en-US") %> -->
   <!-- <%= userBio.birthday %> -->

    <label for="gender">Gender</label>
    <br>
    <input type="text" name="gender" value="<%= userBio.gender %>"/>
    <br>

    <label for="location">Location</label>
    <br>
    <input type="text" name="location" value="<%= userBio.location %>"/>
    <br>

    <label for="phone">Phone Number</label>
    <br>
    <input type="text" name="phone" value="<%= userBio.phone %>"/>
    <br>


    
    <button type="submit">Submit</button>
    </form>
</body>
</html