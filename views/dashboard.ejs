<!DOCTYPE html>
<html>
<head>
    <title>User | All</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
    
</head>
<body>
    <div>
        <div class="row">
            <div class="col">ID</div>
            <div class="col">Username</div>
            <div class="col">User Biodata</div>
            <div class="col"><a href="/user/create">+ CREATE</a></div>

        </div>
    </div>
    <% users.forEach(i => { %>
        <div class="row">
            <div class="col"> <h1><%= i.id %></h1></div>
            <div class="col"> <h1><%= i.username %></h1></div>
            <div class="col"> <h1><a href="/user/<%= i.id %>">Detail</a></h1></div>
            <!-- <div class="col"> <button class="btn-warning btn-lg" style="display: inline-block;"><a href="/user/delete/<%= i.id %>">Delete</a></button></div> -->
            <div class="col">
                <form method="POST" action="/user/delete/<%= i.id %>">
                    <button type="submit">Delete</button>
                </form>
            </div>
        </div><br>
    <% }) %>
</body>
</html>